from collections import defaultdict
class Solution:
    # @param A : list of integers
    # @param B : integer
    # @return an integer
    
    def solve(self, A, B):
        n=len(A)
        xr=0
        mpp=defaultdict(int)
        mpp[xr]=1 #Taki x = xr ^ B 6 ^ 6 = 0 mpp[0] = 1, ek subarray mil gaya
        count=0
        
        for i in range(n):
            xr=xr^A[i]
            x=xr^B
            count+=mpp[x]
            mpp[xr]+=1
        return count
